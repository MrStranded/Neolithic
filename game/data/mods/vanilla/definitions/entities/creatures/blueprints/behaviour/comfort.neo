Drive: driNotSitOnShit {
	Script: condition {
	    require(! .attSleeping);
		require(getInstancesInRange(entShit, self, 0));
	}

	solutions = {
		proStroll;
	}

	Script: getWeight() {
	    return(getInstancesInRange(entShit, self, 0)*100);
	}
}

