pTree {

	onCreate {
		name = "Tree";
		color = [50,150,20];
	
		char = "T";

		ticks = 3;
		growthstage = 1;
	}

	onTick {
		char = growthstage;

		growthstage = growthstage + 1;
		if:(growthstage > 1) {
			char = "G";
		}

		neighbour = getEntity:(0);
		if:(getClass:(neighbour) != "pTree") {
			move:(0);
		}
	}
}