WorldGen: gen01 {
	name: "Desert";
	onStart: worldGen01;

	defaultTileHeight: 100;
	defaultTile: tDirt;

	defaultFluidHeight: 95;
	defaultFluid: fWater;

	formations: mountain01,mountain02,valley01;

	#mountainPercent: "1",42;
	#mountainHeight: 60,255,20,10;

	#valleyPercent: "0.1",100;
	#vallayDepth: 0,10,30,6;
}

Formation: mountain01 {
	spawnPercent: "1",42;
	heightParameters: 200,255,30,10;
}
Formation: mountain02 {
	spawnPercent: "0.5",20;
	heightParameters: 130,170,4,4;
}
Formation: valley01 {
	spawnPercent: "0.1",100;
	heightParameters: 0,50,10,6;
}

WorldGen: gen02 {
	name: "Jungle";
	onStart: worldGen02;
}
